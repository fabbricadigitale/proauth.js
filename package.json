{
  "name": "proauth.js",
  "version": "0.1.0",
  "description": "",
  "private": true,
  "scripts": {
    "dev": "npm run dev:client && npm run dev:legacy && npm run dev:service",
    "dev:client": "BUILD_ENV=develop BUILD_TARGET=client rollup -c",
    "dev:legacy": "BUILD_ENV=develop BUILD_TARGET=legacy rollup -c",
    "dev:service": "BUILD_ENV=devlop BUILD_TARGET=service rollup -c",
    "predev": "npm run clean",
    "lib:client": "BUILD_TARGET=client rollup -c",
    "lib:legacy": "BUILD_TARGET=legacy rollup -c",
    "lib:service": "BUILD_TARGET=service rollup -c",
    "prelib": "npm run clean",
    "lib": "npm run lib:client && npm run lib:legacy && npm run lib:service",
    "test": "exit 1",
    "clean": "shx rm -rf *.log dist/",
    "changelog": "conventional-changelog -p eslint -i CHANGELOG.md -s"
  },
  "bug": {
    "url": "https://gitlab.fabbricadigitale.it/oil/proauth.js/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@ssh.gitlab.fabbricadigitale.it:oil/proauth.js.git"
  },
  "git": {
    "scripts": {
      "commit-msg": "./node_modules/.bin/validate-commit-msg --preset eslint $1"
    }
  },
  "keywords": [
    "auth",
    "client"
  ],
  "license": "MIT",
  "devDependencies": {
    "conventional-changelog-cli": "^1.2.0",
    "git-scripts": "^0.2.1",
    "rollup": "^0.37.0",
    "shx": "^0.2.1",
    "validate-commit": "^2.3.2"
  }
}
